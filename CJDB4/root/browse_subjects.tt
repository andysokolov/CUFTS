[%# PROCESS page_title.tt %]
[% PROCESS browse_form.tt %]
<div id="browse">

[% IF pager.total_entries == 0 %]
 [% c.loc('No results found.') | html %]
[% END %]

[% PROCESS pager.tt %]

[% WHILE ( subject = subjects_rs.next ) %]

<div class="subject">
	<a href="[% c.uri_for_site( c.controller('Browse').action_for('bylink'), 'subject', subject.id ) %]">[% subject.subject | html %]</a>
</div>

[% END %]

[% PROCESS pager.tt %]
</div>