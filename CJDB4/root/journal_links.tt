<div id="links" class="links">
  <!-- <h3>Availability</h3> -->
  <ul>
 [%- SET count = 0 -%]
 [%- FOREACH link IN rank_name_sort( journal.display_links, resources_display ) -%]

  [%- SET resource = undef -%]
  [%- IF link.link_type > 0 -%]
    [% SET resource_id = link.resource;
      SET resource = resources_display.$resource_id %]
    [% NEXT UNLESS resource %]
  [%- END -%]
  [%- SET count = count + 1 -%]

   <li>
      <h4>[%- PROCESS journals_link_name.tt -%]</h4>
      <div class="links-details">
        <dl class="dl-horizontal">

          [% PROCESS journal_coverages.tt %]

          [%- IF link.journal_cjdb_note %]
              <dt class="journal-note note">Journal Note:</dt>
              <dd class="journal-note note">[% link.journal_cjdb_note | html %]</dd>
          [%- END -%]

          [%- IF resource.cjdb_note %]
            <dt class="resource-note note">Resource Note:</dt>
            <dd class="resource-note note">[% resource.cjdb_note | html %]</dd>
          [%- END %]

        </dl>
        <a class="btn btn-medium btn-primary" href="[% link.url | html %]">Access Journal</a>

        [% PROCESS journal_license.tt %]

      </div>

   </li>
 [%- END -%]
  </ul>
 [% IF count == 0 %]
  No links are currently available for this journal.
 [% END %]
</div>

